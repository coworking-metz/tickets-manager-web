    "@unhead/vue": "^1.8.8",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.2",
    "axios-retry": "^3.9.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "typeface-inter": "^3.18.1",
    "uuid": "^9.0.1",
    "vue": "^3.3.9",
